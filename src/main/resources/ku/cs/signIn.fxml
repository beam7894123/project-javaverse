      <Label fx:id="signInLabel" layoutX="349.0" layoutY="76.0" text="SignIn">
         <font>
            <Font name="Constantia" size="36.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="189.0" layoutY="224.0" text="Username :">
         <font>
            <Font name="Georgia" size="18.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="192.0" layoutY="327.0" text="Password :">
         <font>
            <Font name="Georgia Pro" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordPasswordfield" layoutX="311.0" layoutY="325.0" prefHeight="28.0" prefWidth="202.0" />
      <TextField fx:id="usernameTextfield" layoutX="311.0" layoutY="221.0" prefHeight="28.0" prefWidth="202.0" />
      <Button layoutX="610.0" layoutY="445.0" mnemonicParsing="false" onAction="#handleConfirmButtonClick" prefHeight="35.0" prefWidth="102.0" text="Confirm" />
